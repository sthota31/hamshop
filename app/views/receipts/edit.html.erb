<div class="container-fluid" style="padding-top:50px">
    <div class="row-fluid">
        <div class="span9">
<h1>Editing receipt</h1>
<ul>
      <% @receipt.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>


<%= form_for @receipt, :url => receipt_path(@receipt), :html => { :multipart => true } do |f| %>
<%= render :partial => 'form', :locals => {:f => f} %> 
<% end %>

<%= link_to 'Back', receipts_path %>
</div>
</div>
</div>